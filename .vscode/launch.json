    "version": "0.2.0",
    "configurations": [
        {
            "name": "BiRex (debug)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "console": "debugConsole",
            "toolArgs": [
                "--dart-define-from-file",
                "${workspaceFolder}/.env",
            ],
        },
        {
            "name": "BiRex (release)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                "${workspaceFolder}/.env",
            ],
        },
    ]
}